{
  "name": "sicpjs",
  "version": "1.0.0",
  "description": "infrastructure for generating SICP JS",
  "keywords": [
    "JavaScript",
    "Source",
    "SICP",
    "SICP JS"
  ],
  "author": {
    "name": "Source Academy",
    "url": "https://github.com/source-academy/"
  },
  "license": "Apache 2.0",
  "main": "index.js",
  "dependencies": {
    "fs-extra": "^9.0.0",
    "lz-string": "^1.4.4",
    "xmldom": "^0.1.27",
    "xpath": "0.0.27"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "husky": "^4.2.5",
    "js-slang": "^0.4.33",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "check": "prettier --list-different \"js_programs/**/*.js\"",
    "format": "prettier --write ./javascript/*.js ./javascript/*/*.js",
    "start": "babel-node ./javascript/index.js",
    "test": "./scripts/test.sh"
  },
  "husky": {
    "hooks": {
	"pre-commit": "yarn tslint --fix && yarn format && yarn tslint",
	"pre-push": "yarn ci"
    }
  }
}
